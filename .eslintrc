{
  "parser": "babel-eslint",
  "extends": [
    "airbnb"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "class-methods-use-this": "off",
    "no-param-reassign": "off",
    "no-new": "off",
    "no-plusplus": "off",
    "consistent-return": "off",
    "global-require": "off"
  }
}